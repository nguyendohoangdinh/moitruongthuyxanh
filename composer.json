{
  "require": {
    "acoustep/craft-easymde": "0.1.7",
    "barrelstrength/sprout-forms": "3.9.0.1",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "^3.0.0",
    "craftcms/redactor": "2.4.0",
    "ether/simplemap": "3.8.3",
    "verbb/icon-picker": "1.1.1",
    "verbb/super-table": "2.3.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
